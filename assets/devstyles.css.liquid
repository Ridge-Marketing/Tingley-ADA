.nunito {
  font-family: "Nunito Sans";
  line-height: 30px; }

.nunito-title {
  font-weight: 900;
  font-family: "Nunito Sans";
  font-size: 2.25rem; }

.nunito-subtitle {
  font-size: 24px;
  line-height: 30px;
  font-weight: 900;
  font-family: 'Nunito Sans'; }

.nunito-extrabold {
  font-weight: 900; }

.hide--mobile, .hide--mobile-inline {
  display: none; }

.product--wrapper {
  padding: 2rem 0; }

.product--fit-type {
  padding-top: 8px;
  font-size: 16px;
  display: inline-block;
  margin-bottom: 0; }

.product--specs {
  display: flex;
  justify-content: space-between;
  margin-bottom: 1rem; }

.quantity-select {
  display: block;
  float: left;
  width: 100%; }

.product--link-small {
  font-size: 14px;
  display: inline-block;
  text-decoration: underline;
  color: #4A4A4A;
  font-weight: 900; }

.product-details--container {
  padding-top: 32px; }

.product-details--container ul {
  list-style: disc outside;
  padding-left: 20px; }

.product-details--container ul li {
  padding: 4px 0;
  color: #545454; }
.product-details--container a {
  color: #545454;
  text-decoration: underline; }
.product-details--container a:hover {
  color: black; }

.additional-info--container a {
  /* change to block if we're not using my arrow method  */
  display: inline-block;
  color: #545454;
  text-decoration: underline;
  font-weight: 900; }
.additional-info--container h6 {
  font-size: 12px;
}
#Thumbnails {
  grid-template-columns: repeat(3,1fr);
} 
@media (min-width: 1024px) {
  #Thumbnails {
    grid-template-columns: repeat(4,1fr);
  }  
}
@media (min-width: 1280px) {
  #Thumbnails {
    grid-template-columns: repeat(5,1fr);
  }  
}
@media (min-width: 1536px) {
  #Thumbnails {
    grid-template-columns: repeat(6,1fr);
  }  
}

@media (min-width: 768px) {
  .product--wrapper {
    padding: 2rem 1rem; }
  .product--specs {
    display: inline;
    padding-left: 1rem; }
  .product--fit-type {
    margin-bottom: 1rem; }
  .hide--desktop {
    display: none; }
  .hide--mobile {
    display: block; }
  .hide--mobile-inline {
    display: inline-block; }
  .product-details--container ul {
    display: flex;
    flex-wrap: wrap;
    padding-left: 0; }
  .product-details--container ul li {
    width: 45%;
    margin: 0 2.5%;
    padding-right: 70px; } }

.product-card--container {
  /* width: 100%;  */
  padding: 1rem;
  border: 1px solid #DDDDDD; }

.product-card--img {
  margin: 0 auto;
  width: 9rem;
  height: 12rem; }

.product-card--price {
  color: #EA1821;
  font-size: 20px; }

.product-card--title {
  font-size: 13px;
  line-height: 20px;
  color: #545454; }

.button-prev, .button-next {
  display: flex;
  margin: 0 10px;
  align-items: center; }

.swiper-container {
  width: 85%;
  height: 100%; }

@media (min-width: 1536px) {
  .button-next, .button-prev {
    display: none;
    height: 0;
    width: 0;
    margin: 0; }
  .swiper-container {
    width: 99%; } }

.spec--wrapper a {
  font-size: 14px;
  display: inline-block;
  text-decoration: underline;
  color: #4A4A4A;
  font-weight: 900;
  text-transform: uppercase; }

.swiper-slide {
  height: auto; }

.sm-text-center {
  text-align: center; }

@media (min-width: 768px) {
  .sm-text-center {
    text-align: left; } }

.icon-container {
  padding: 0 36px; }

.lg-grid {
  text-align: center;
  margin-top: 36px; }

.lg-grid .indv-icon {
  margin-bottom: 40px; }

.lg-grid #icon-text h3 {
  margin-top: 36px; }

@media (min-width: 768px) {
  .lg-grid {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 20px 24px;
    text-align: left; }
  .lg-grid .indv-icon {
    display: flex;
    margin: 0; }
  .lg-grid #icon-text {
    padding-left: 24px; }
  .lg-grid #icon-text h3 {
    margin-top: 0; } }

@media (min-width: 1024px) {
  .lg-grid {
    gap: 20px 80px; } }

@media (min-width: 768px) {
  .icon-group {
    border-top: 1px solid #e5e5e5; } }

